    $.fn.enterKey = function (fnc) {
        return this.each(function () {
            $(this).keypress(function (ev) {
                var keycode = (ev.keyCode ? ev.keyCode : ev.which);
                if (keycode == '13') {
                    fnc.call(this, ev);
                }
            })
        })
    };

    (function($){
        $('.add_gift_wrap').click(function(e){
            var $t = $(this);
            e.preventDefault();
            $t.attr('disabled','disabled');

            var url = '';
            var complement = '?sku=' + $(this).parents('tr').find('.product_sku').text();
            if ($(this).is(':checked')) {
                url = 'https://origin-www.dafiti.com.br/cart/addgiftwrap/';
                complement += '&amp;free='+$(this).hasClass('telesales');
            } else {
                url = 'https://origin-www.dafiti.com.br/cart/removegiftwrap/';
            }
            window.location = url + complement ;

        });
        $('#cep').mask('99999-999');
        $('#cep').enterKey(function (obj) {
            obj.preventDefault();
            getFreightOptions();
        });
    })(jQuery);

    function getFreightOptions()
    {
        if ($('#cep').val() == "") {
            return;
        }

        $('.inputs-cep .spinner').show();
        $.get('/cart/GetFreightOptions?postcode=' + $('#cep').val(), function(data) {
            $("#freight-options-holder").html(data);

            $('.inputs-cep .spinner').hide();
        });
    }

    function getFreightOptionsSplitedFields()
    {
        if ($('#cep-begin').val() == "") {
            return;
        }
        if ($('#cep-final').val() == "") {
            return;
        }
        $('.inputs-cep .spinner').show();
        $.get('/cart/GetFreightOptions?postcode=' + $('#cep-begin').val() + "-" + $('#cep-final').val(), function(data) {
            $("#freight-options-holder").html(data);

            $('.inputs-cep .spinner').hide();
        });
    }

    function loadValues()
    {
        var checked = $('input[name=frete_option]:checked');
        checked.parent().find('.spinner').show();
        var cep = checked.parent().find('input[name=cep]').val();
        var query = '?postcode=' + cep + '&amp;delivery_type=' + checked.val();
        $.get('/cart/GetTotalWithFreight' + query, function (data) {
            $('.sel-shipping-free').html(data.freight);
            $('.sel-total').html(data.total);
            $('.installments-times').html(data.installmentTimes);
            $('.installments-total').html(data.installmentValue);
            checked.parent().find('.spinner').hide();
        }, 'json');
    }
